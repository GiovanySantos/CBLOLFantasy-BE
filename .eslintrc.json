{
  "root": true,
  "extends": "airbnb-base",
  "env": {
    "jest": true,
    "node": true
  },
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2023,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "import/extensions": 0,
    "no-console": "off",
    "no-unused-vars": "off",
    "no-shadow": "off"
  }
}
